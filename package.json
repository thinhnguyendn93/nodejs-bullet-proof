  "devDependencies": {
    "@types/agenda": "2.0.9",
    "@types/express": "4.17.8",
    "@types/jest": "29.5.3",
    "@types/lodash": "4.14.161",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.6.2",
    "nodemon": "3.0.1",
    "prettier": "3.0.0",
    "ts-jest": "26.4.0",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.11.0",
    "tslint": "5.20.1",
    "typescript": "5.1.6"
  }
